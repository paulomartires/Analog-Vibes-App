# Production Environment Variables for Vercel Deployment
# These should be set in the Vercel dashboard, not in this file

# Supabase Configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=

# Discogs API Configuration
VITE_DISCOGS_CONSUMER_KEY=
VITE_DISCOGS_CONSUMER_SECRET=
VITE_DISCOGS_PERSONAL_ACCESS_TOKEN=

# API Settings
VITE_DISCOGS_API_BASE_URL=https://api.discogs.com
VITE_DISCOGS_USER_AGENT=AnalogVibesApp/1.0

# Rate Limiting
VITE_DISCOGS_RATE_LIMIT_PER_MINUTE=60
VITE_DISCOGS_RATE_LIMIT_AUTHENTICATED=55

# Your Discogs Username
VITE_DISCOGS_USERNAME=

# Production Configuration
VITE_APP_ENV=production
VITE_ENABLE_REAL_TIME=true
VITE_OFFLINE_CACHE_HOURS=24